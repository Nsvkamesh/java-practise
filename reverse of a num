import java.util.*;

public class Main {
    public static void main(String args[]) {
        int a;
        int b;
        int rem = 0;
        Scanner sc = new Scanner(System.in);
        a = sc.nextInt();
        while (a > 0) {
            b = a % 10;
            rem = rem * 10 + b;
            a = a / 10;
        }
        System.out.println(rem);
    }
}
/reverse of num
